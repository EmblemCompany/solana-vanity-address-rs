{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cargo build --release --features api --bin solana-vanity-api"
  },
  "deploy": {
    "startCommand": "./target/release/solana-vanity-api",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}